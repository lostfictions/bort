{
  "name": "bort",
  "version": "2.0.0",
  "description": "a fun bot for you",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start-dev": "DEBUG=\"bort:*\" DEBUG_HIDE_DATE=true DEBUG_COLORS=true nodemon -i persist dist/main.js",
    "dev": "run-p -l watch:ts start-dev",
    "dev-cli": "USE_CLI=true npm run dev",
    "build": "rm -rf dist/ && tsc",
    "test": "jest",
    "watch:ts": "tsc -w",
    "watch:test": "jest --colors --watch"
  },
  "author": "s",
  "license": "GPL3",
  "dependencies": {
    "@slack/client": "^3.14.2",
    "chatter": "^0.5.1",
    "cheerio": "^0.22.0",
    "cmu-pronouncing-dictionary": "^1.0.3",
    "debug": "^3.1.0",
    "discord.js": "^11.2.1",
    "envalid": "^4.1.1",
    "express": "^4.16.2",
    "got": "^7.1.0",
    "immutable": "^3.8.2",
    "jimp": "^0.2.28",
    "moment": "^2.19.2",
    "react": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-immutable": "^3.1.0",
    "source-map-support": "^0.5.0",
    "syllable": "^2.2.0",
    "validator": "^8.1.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.5",
    "@types/debug": "0.0.30",
    "@types/express": "^4.0.39",
    "@types/express-serve-static-core": "^4.0.56",
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.53",
    "@types/redux-immutable": "^3.0.38",
    "@types/validator": "^6.3.0",
    "eslint": "^4.11.0",
    "jest": "^21.2.1",
    "jsverify": "^0.8.3",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "ts-jest": "^21.2.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  },
  "engines": {
    "node": ">=8.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
