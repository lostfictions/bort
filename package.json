{
  "name": "bort",
  "version": "6.0.2",
  "description": "a fun bot for you",
  "private": true,
  "main": "dist/main.js",
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "NODE_ENV=development DEBUG=bort DEBUG_HIDE_DATE=true DEBUG_COLORS=true ts-node -T src/main.ts",
    "dev-cli": "USE_CLI=true run-s dev",
    "dev-testing": "USE_TESTING=true run-s dev",
    "build": "run-s -l build:*",
    "build:clean": "rm -rf dist/",
    "build:ts": "tsc",
    "build:next": "next build",
    "test": "run-s -l test:*",
    "test:ts": "tsc --noEmit",
    "test:eslint": "eslint 'src/**/*'",
    "test:jest": "jest --colors",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write",
    "jest": "jest --colors --watch"
  },
  "dependencies": {
    "@sentry/integrations": "^5.29.0",
    "@sentry/node": "5.29.0",
    "axios": "^0.21.0",
    "cheerio": "^0.22.0",
    "chrono-node": "^2.1.10",
    "cmu-pronouncing-dictionary": "^1.0.3",
    "common-tags": "^1.8.0",
    "cookie": "^0.4.1",
    "dayjs": "^1.9.7",
    "debug": "^4.3.1",
    "discord.js": "^12.5.1",
    "envalid": "6.0.1",
    "jsonwebtoken": "^8.5.1",
    "level": "^6.0.1",
    "next": "^10.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "source-map-support": "^0.5.19",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.22",
    "@types/common-tags": "^1.8.0",
    "@types/cookie": "^0.4.0",
    "@types/debug": "4.1.5",
    "@types/jest": "^26.0.17",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/levelup": "^4.3.0",
    "@types/node": "^14.14.10",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/uuid": "^8.3.0",
    "astroturf": "^0.10.5",
    "eslint": "^7.15.0",
    "eslint-config-lostfictions": "^0.5.0",
    "jest": "^26.6.3",
    "jsverify": "^0.8.4",
    "next-compose-plugins": "^2.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=12.1.0"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "rootDir": "src",
    "globals": {
      "ts-jest": {
        "isolatedModules": true
      }
    }
  }
}
