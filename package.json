{
  "name": "bort",
  "version": "5.1.1",
  "description": "a fun bot for you",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "DEBUG=bort DEBUG_HIDE_DATE=true DEBUG_COLORS=true ts-node -T src/main.ts",
    "dev-cli": "USE_CLI=true run-s dev",
    "build": "rm -rf dist/ && tsc",
    "test": "run-s -l test:*",
    "test:ts": "tsc --noEmit",
    "test:eslint": "eslint 'src/**/*'",
    "test:jest": "jest --colors",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write",
    "jest": "jest --colors --watch"
  },
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "dependencies": {
    "@sentry/integrations": "^5.12.0",
    "@sentry/node": "5.12.3",
    "axios": "^0.19.2",
    "cheerio": "^0.22.0",
    "cmu-pronouncing-dictionary": "^1.0.3",
    "common-tags": "^1.8.0",
    "dayjs": "^1.8.20",
    "debug": "^4.1.1",
    "discord.js": "^11.5.1",
    "envalid": "^6.0.1",
    "level": "^5.0.1",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/cheerio": "^0.22.16",
    "@types/common-tags": "^1.8.0",
    "@types/debug": "4.1.5",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "jest": "^25.1.0",
    "jsverify": "^0.8.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "engines": {
    "node": ">=12.1.0"
  },
  "jest": {
    "rootDir": "src"
  }
}
