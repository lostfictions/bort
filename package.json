{
  "name": "bort",
  "version": "6.0.3",
  "description": "a fun bot for you",
  "private": true,
  "main": "dist/main.js",
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "node --enable-source-maps dist/main.js",
    "dev": "DEBUG=bort DEBUG_HIDE_DATE=true DEBUG_COLORS=true ts-node -T src/main.ts",
    "dev-cli": "USE_CLI=true run-s dev",
    "build": "rm -rf dist/ && tsc --pretty",
    "test": "run-p -cl test:*",
    "test:ts": "tsc --pretty --noEmit",
    "test:eslint": "eslint --color --max-warnings=0 'src/**/*'",
    "test:jest": "jest --colors",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write",
    "jest": "jest --watch"
  },
  "dependencies": {
    "@sentry/integrations": "^7.109.0",
    "@sentry/node": "7.109.0",
    "axios": "^0.27.2",
    "cheerio": "^0.22.0",
    "chrono-node": "^2.7.5",
    "cmu-pronouncing-dictionary": "^1.0.3",
    "common-tags": "^1.8.2",
    "dayjs": "^1.11.10",
    "debug": "^4.3.4",
    "discord.js": "^12.5.3",
    "level": "^7.0.1",
    "lru-cache": "^6.0.0",
    "znv": "^0.4.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cheerio": "0.22.22",
    "@types/common-tags": "^1.8.4",
    "@types/debug": "4.1.12",
    "@types/jest": "^29.5.12",
    "@types/levelup": "^4.3.3",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^20.12.4",
    "dotenv": "^14.3.2",
    "eslint": "^8.57.0",
    "eslint-config-lostfictions": "^6.1.0",
    "jest": "^29.7.0",
    "jsverify": "^0.8.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "rootDir": "src",
    "transform": {
      "^.+\\.[tj]sx?$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    }
  }
}
