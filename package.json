{
  "name": "bort",
  "version": "4.0.0",
  "description": "a fun bot for you",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "start-dev": "DEBUG=\"bort:*\" DEBUG_HIDE_DATE=true DEBUG_COLORS=true nodemon -i persist dist/main.js",
    "dev": "run-p -l watch:ts start-dev",
    "dev-cli": "USE_CLI=true npm run dev",
    "build": "rm -rf dist/ && tsc",
    "prettier": "prettier 'src/**/*' --write",
    "test": "run-s -l test:*",
    "test:jest": "jest --colors",
    "test:ts": "tsc --noEmit",
    "test:eslint": "eslint 'src/**/*'",
    "watch:ts": "tsc -w",
    "watch:jest": "jest --colors --watch"
  },
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "dependencies": {
    "cheerio": "^0.22.0",
    "cmu-pronouncing-dictionary": "^1.0.3",
    "common-tags": "^1.8.0",
    "debug": "^3.1.0",
    "discord.js": "^11.3.2",
    "envalid": "^4.1.4",
    "express": "^4.16.3",
    "got": "^8.3.1",
    "immutable": "^3.8.2",
    "jimp": "^0.2.28",
    "moment": "^2.22.2",
    "react": "^16.4.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "source-map-support": "^0.5.6",
    "validator": "^10.3.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/common-tags": "^1.4.0",
    "@types/debug": "0.0.30",
    "@types/express": "^4.16.0",
    "@types/express-serve-static-core": "^4.16.0",
    "@types/got": "^8.3.1",
    "@types/jest": "^23.0.0",
    "@types/node": "^10.3.1",
    "@types/redux-immutable": "^3.0.38",
    "@types/validator": "^9.4.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-typescript": "^0.12.0",
    "jest": "^23.1.0",
    "jsverify": "^0.8.3",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.4",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.1",
    "typescript-eslint-parser": "^16.0.0"
  },
  "engines": {
    "node": ">=10.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
