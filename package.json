{
  "name": "bort",
  "version": "7.0.0",
  "description": "a fun bot for you",
  "private": true,
  "main": "dist/main.js",
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "type": "module",
  "scripts": {
    "start": "DEBUG=bort node --experimental-strip-types src/main.ts",
    "dev": "DEBUG=bort DEBUG_HIDE_DATE=true DEBUG_COLORS=true node --experimental-strip-types src/main.ts",
    "dev-cli": "USE_CLI=true run-s dev",
    "test": "run-p -cl test:*",
    "test:ts": "tsc --pretty --noEmit",
    "test:eslint": "eslint --color --max-warnings=0 'src/**/*'",
    "test:vitest": "vitest run --color",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write",
    "vitest": "vitest"
  },
  "dependencies": {
    "@sentry/node": "8.45.0",
    "cheerio": "^1.0.0",
    "chrono-node": "^2.7.7",
    "cmu-pronouncing-dictionary": "^1.0.3",
    "common-tags": "^1.8.2",
    "dayjs": "^1.11.13",
    "debug": "^4.4.0",
    "discord.js": "^14.16.3",
    "ky": "^1.7.3",
    "level": "^7.0.1",
    "znv": "^0.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.4",
    "@types/debug": "4.1.12",
    "@types/levelup": "^4.3.3",
    "@types/node": "^22.10.2",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "eslint-config-lostfictions": "7.0.0-alpha.7",
    "fast-check": "^3.23.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "packageManager": "pnpm@9.7.1+sha512.faf344af2d6ca65c4c5c8c2224ea77a81a5e8859cbc4e06b1511ddce2f0151512431dd19e6aff31f2c6a8f5f2aced9bd2273e1fed7dd4de1868984059d2c4247"
}
