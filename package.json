{
  "name": "bort",
  "version": "5.1.1",
  "description": "a fun bot for you",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "DEBUG=bort DEBUG_HIDE_DATE=true DEBUG_COLORS=true ts-node-dev --transpileOnly --notify=false src/main.ts",
    "dev-cli": "USE_CLI=true npm run dev",
    "build": "rm -rf dist/ && tsc",
    "test": "run-s -l test:*",
    "test:ts": "tsc --noEmit",
    "test:eslint": "eslint 'src/**/*'",
    "test:jest": "jest --colors",
    "test:prettier": "prettier 'src/**/*' -l",
    "prettier": "prettier 'src/**/*' --write",
    "jest": "jest --colors --watch"
  },
  "author": "s <https://github.com/lostfictions>",
  "license": "AGPL-3.0",
  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^0.22.0",
    "cmu-pronouncing-dictionary": "^1.0.3",
    "common-tags": "^1.8.0",
    "debug": "^4.1.1",
    "discord.js": "^11.5.1",
    "envalid": "^5.0.0",
    "express": "^4.17.1",
    "level": "^5.0.1",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "source-map-support": "^0.5.12",
    "validator": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/cheerio": "^0.22.11",
    "@types/common-tags": "^1.8.0",
    "@types/debug": "4.1.4",
    "@types/express": "^4.16.1",
    "@types/express-serve-static-core": "^4.16.6",
    "@types/jest": "^24.0.13",
    "@types/node": "^12.0.8",
    "@types/validator": "^10.11.1",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "jest": "^24.8.0",
    "jsverify": "^0.8.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.1"
  },
  "engines": {
    "node": ">=12.1.0"
  }
}
